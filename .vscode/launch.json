{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/main.exe",
			"args": ["random", "{\\\"particles\\\":[[\\\"e\\\",4],[\\\"q\\\",4],[\\\"p\\\",4]],\\\"blocks\":[[\\\"ee->pp\\\",2,[\\\"e\",\\\"e\\\"],[\\\"p\\\",\\\"p\\\"]],[\\\"qq->\\\",2,[\\\"q\\\",\\\"q\\\"],[]],[\\\"p->qe\\\",2,[\\\"p\\\"],[\\\"q\\\",\\\"e\\\"]],[\\\"eq->p\\\",2,[\\\"e\",\\\"q\\\"],[\"p\"]]]}"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			//"miDebuggerPath": "/path/to/gdb",
			"preLaunchTask": "build",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		},
	]
}
