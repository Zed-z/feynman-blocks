{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Compile and run idk",
            "command": "gcc",
            "args": [
                "-fdiagnostics-color=always",

                "-g",
                "${fileDirname}\\**.c",
                "${fileDirname}\\source\\**.c",

                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",

                //"-I/mingw64/include",
                //"-L/mingw64/lib",
                //"-L/C/msys64/usr/bin",
                //"-lmsys-jansson-4.dll"
                //"-I", "${fileDirname}\\lib",
                //"-L", "${fileDirname}\\lib",
                //"-llibjansson-4"
            ],
            "options": {
                "cwd": "${fileDirname}",
                "env": {
                    "PATH": "${env:PATH};C:\\msys64\\usr\\bin"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}